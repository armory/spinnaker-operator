{
  "apiVersion": "spinnaker.io/v1alpha2",
  "kind": "SpinnakerService",
  "metadata": {
    "name": "spinnaker",
    "namespace": "ns1"
  },
  "spec": {
    "expose": {
      "service": {
        "overrides": {},
        "publicPort": 80,
        "type": "LoadBalancer"
      },
      "type": "service"
    },
    "spinnakerConfig": {
      "config": {
        "deploymentEnvironment": {
          "accountName": "kubernetes",
          "size": "SMALL",
          "type": "Distributed"
        },
        "name": "default",
        "persistentStorage": {
          "azs": {},
          "gcs": {
            "rootFolder": "front50"
          },
          "oracle": {},
          "persistentStoreType": "s3",
          "redis": {},
          "s3": {
            "bucket": "german-spinnaker",
            "region": "us-west-2",
            "rootFolder": "front50"
          }
        },
        "version": "1.15.1",
        "security": {
          "apiSecurity": {
            "ssl": {
              "enabled": false
            }
          },
          "uiSecurity": {
            "ssl": {
              "enabled": false
            }
          }
        }
      },
      "profiles": {
        "gate": {
          "default": {
            "apiPort": 8085
          }
        }
      }
    }
  },
  "status": {
    "apiUrl": "http://acme.com",
    "uiUrl": "http://acme.com"
  }
}
